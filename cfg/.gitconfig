[user]
	name = Daniel Gryniewicz
	email = dang@fprintf.net
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	func = blue bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[branch]
	autosetupmerge = always
	autosetuprebase = always
[push]
	default = matching
[core]
	excludesfile = "~/.gitignore"
	whitespace = trailing-space,space-before-tab
[svn]
	rmdir = true
[diff]
	renamelimit = 1500
	renames = copies
[alias]
	ls = show --pretty="format:" --name-only
	patch = apply --stat --reject --apply
[apply]
	whitespace = fix
